LC_ALL=C \
PATH=/bin \
HOME=/home/test \
USER=test \
LOGNAME=test \
QEMU_AUDIO_DRV=none \
/usr/bin/qemu \
-name SomeDummyHugepagesGuest \
-S \
-M pc \
-m 1024 \
-mem-prealloc \
-mem-path /dev/hugepages2M/libvirt/qemu \
-smp 2 \
-uuid ef1bdff4-27f3-4e85-a807-5fb4d58463cc \
-nographic \
-monitor unix:/tmp/test-monitor,server,nowait \
-no-acpi \
-boot c \
-usb \
-drive file=/dev/HostVG/QEMUGuest1,format=raw,if=ide,bus=0,unit=0 \
-net none \
-serial none \
-parallel none
